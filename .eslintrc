{
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "plugins": [
        "import"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "vars": "all",
                "args": "after-used",
                "ignoreRestSiblings": true
            }
        ],
        "no-console": "warn",
        "no-eval": "error",
        "import/first": "error",
        "prefer-const": [
            "error",
            {
                "destructuring": "all"
            }
        ]
    }
}